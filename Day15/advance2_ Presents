#include <iostream>
#include <vector>

using namespace std;

int main() {
    int n;
    cin >> n;
    vector<int> giver(n + 1); 
    
    for (int i = 1; i <= n; ++i) {
        int receiver;
        cin >> receiver;
        giver[receiver] = i;
    }

    for (int i = 1; i <= n; ++i) {
        cout << giver[i] << (i == n ? "" : " ");
    }
    cout << endl;
    return 0;
}
