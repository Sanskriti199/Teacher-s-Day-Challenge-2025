#include <iostream>
#include <cmath>

using namespace std;

int main() {
    int row_one = 0, column_one = 0;
    int value;

    for (int i = 1; i <= 5; ++i) {
        for (int j = 1; j <= 5; ++j) {
            cin >> value;
            if (value == 1) {
                row_one = i;
                column_one = j;
            }
        }
    }

    int moves = abs(row_one - 3) + abs(column_one - 3);
    cout << moves << endl;

    return 0;
}
