#include<bits/stdc++.h>
using namespace std;

int main(){
    string a1,a2;
    cin>>a1>>a2;
    
    for(int i=0;i<a1.size();i++){
        if(a1[i]<92){
            a1[i]+=32;
        }
        if(a2[i]<92){
            a2[i]+=32;
        }
    }
    if(a1<a2){
        cout<<-1;
    }
    if(a1>a2){
        cout<<1;
    }
    if(a1==a2){
        cout<<0;
    }
    return 0;
}
